{
  "name": "@newy/define",
  "version": "0.0.1",
  "author": "weisongyao",
  "license": "MIT",
  "type": "module",
  "main": "dist/define.cjs.js",
  "module": "dist/define.esm-bundler.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/define.cjs.js",
      "module": "./dist/define.esm-bundler.js",
      "import": "./dist/define.esm-bundler.js",
      "require": "./dist/define.cjs.js"
    },
    "./*": "./*"
  },
  "buildOptions": {
    "formats": [
      "esm-bundler",
      "cjs"
    ],
    "copy": "public/define.d.ts"
  },
  "sideEffects": false
}

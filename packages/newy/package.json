{
  "name": "newy",
  "version": "0.0.2",
  "author": "weisongyao",
  "keywords": [
    "newy"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/newy.esm-bundler.js",
  "types": "types/index.d.ts",
  "unpkg": "dist/newy.global.js",
  "jsdelivr": "dist/newy.global.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "types"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "node": "./dist/newy.cjs.js",
      "module": "./dist/newy.esm-bundler.js",
      "import": "./dist/newy.esm-bundler.js",
      "require": "./dist/newy.cjs.js"
    },
    "./*": "./*"
  },
  "dependencies": {
    "@newy/define": "workspace:^",
    "@newy/html": "workspace:^",
    "@newy/signal": "workspace:^",
    "@newy/plugin-signal-extend": "workspace:^"
  },
  "buildOptions": {
    "name": "Newy",
    "formats": [
      "esm-bundler",
      "esm-browser",
      "cjs",
      "global"
    ]
  },
  "sideEffects": false
}

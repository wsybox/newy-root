{
  "name": "@newy/shared",
  "version": "0.0.4",
  "author": "weisongyao",
  "license": "MIT",
  "type": "module",
  "main": "dist/shared.cjs.js",
  "module": "dist/shared.esm-bundler.js",
  "types": "types/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "types"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "node": "./dist/shared.cjs.js",
      "module": "./dist/shared.esm-bundler.js",
      "import": "./dist/shared.esm-bundler.js",
      "require": "./dist/shared.cjs.js"
    },
    "./*": "./*"
  },
  "buildOptions": {
    "formats": [
      "esm-bundler",
      "cjs"
    ]
  },
  "sideEffects": false
}

{
  "name": "@newy/signal",
  "version": "0.0.1",
  "description": "@newy/signal",
  "author": "weisongyao",
  "keywords": [
    "newy",
    "signal"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/signal.esm-bundler.js",
  "types": "types/index.d.ts",
  "unpkg": "dist/signal.global.js",
  "jsdelivr": "dist/signal.global.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "node": "./dist/signal.cjs.js",
      "module": "./dist/signal.esm-bundler.js",
      "import": "./dist/signal.esm-bundler.js",
      "require": "./dist/signal.cjs.js"
    },
    "./*": "./*"
  },
  "dependencies": {
    "@newy/shared": "workspace:^"
  },
  "buildOptions": {
    "name": "NewySignal",
    "formats": [
      "esm-bundler",
      "esm-browser",
      "cjs",
      "global"
    ]
  },
  "sideEffects": false
}
